				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>RETURNS_VALUE</ReturnModificator>
					<ReturnType><![CDATA[int]]></ReturnType>
					<Id>1576819059792</Id>
					<Name><![CDATA[getRandomState]]></Name>
					<X>300</X><Y>370</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[// find any state but not where the flag is.

int location = uniform_discr(0, main.MAP_R * main.MAP_C -1);

int flag = main.flag.r * main.MAP_C + main.flag.c;

while (location == flag) {
	location = uniform_discr(0, main.MAP_R * main.MAP_C -1);
}

return location;]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>RETURNS_VALUE</ReturnModificator>
					<ReturnType><![CDATA[int]]></ReturnType>
					<Id>1576819616448</Id>
					<Name><![CDATA[argMaxQ]]></Name>
					<X>300</X><Y>400</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[state]]></Name>
						<Type><![CDATA[int]]></Type>
					</Parameter>
					<Body><![CDATA[int argMax_action = 0;

double mx = Q_Table[state][argMax_action];

for (int j=1; j < NUMBER_OF_ACTIONS; j++){
	if (Q_Table[state][j] > mx){
		mx = Q_Table[state][j];	
		argMax_action = j;
	} 
}

return argMax_action;
]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1576820555365</Id>
					<Name><![CDATA[startEpisode]]></Name>
					<X>60</X><Y>550</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[if (isNewEpisode) {
	done = false;
	total_reward = 0; //reset
	reward = 0;

	state = getRandomState();
	
	// move the player
	int r = getStateToRC(state)[0];
	int c = getStateToRC(state)[1];
	main.player.moveTo(r, c);
	
	isNewEpisode = false;
	episode ++;
	
	
	if (episode % 500 == 0){
		//traceln("------" + episode + "-------");
		//printQ();
	}
}

if (episode > EPISODES)
	getEngine().pause();
]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1576821253941</Id>
					<Name><![CDATA[observeAfterStep]]></Name>
					<X>60</X><Y>580</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[// get state
state_new = main.player.r * main.MAP_C + main.player.c;

// if captured the flag
int flag_state = main.flag.r * main.MAP_C + main.flag.c;

double delta = 0;

if (state_new == flag_state){
	reward = 100; // get reward
	total_reward += reward;
	
	Q_Table[state][action] = reward;
	
	done = true;
	
	// endEpisode ;
	isNewEpisode = true;

	EPSILON -= (EPSILON)/EPISODES;
	
	
	avg_rewards.add(total_reward);
	
	//sample avg
	if (episode % 100 == 0) {
		epi_awards.add(episode, avg_rewards.mean());		 
		avg_rewards.reset();
	
	}
	
	// if (episode % 10 == 0)
	//	traceln( "Episode: " + episode + " REWARDs: " + total_reward);
	
} else {
	reward = -10; // get reward
	
	
	delta = LEANRING_RATE * (reward + DISCOUNT * getMaxQ(state_new) - getQ_s_a(state, action));
	
	Q_Table[state][action] = Q_Table[state][action] + delta;
	
	total_reward += reward;
	state = state_new;
}

]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>RETURNS_VALUE</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1576821886026</Id>
					<Name><![CDATA[getMaxQ]]></Name>
					<X>300</X><Y>430</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[state]]></Name>
						<Type><![CDATA[int]]></Type>
					</Parameter>
					<Body><![CDATA[
double m = Q_Table[state][0]; 

int i = 1;

while (i < NUMBER_OF_ACTIONS) {
	if (Q_Table[state][i] >= m){
		m = Q_Table[state][i];
	}
	i++;
}

return m;]]></Body>
