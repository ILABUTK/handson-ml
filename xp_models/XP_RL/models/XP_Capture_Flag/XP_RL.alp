					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[action]]></Name>
						<Type><![CDATA[int]]></Type>
					</Parameter>
					<Body><![CDATA[int r = this.getR();
int c = this.getC();

switch ( action ){
	case 0:
		r--;
		break;
	case 1: 
		r++;
		break;
	case 2:
		c--;
		break;
	case 3:
		c++;
		break;	
}

if (r<0) return;
if (c<0) return;
if (r>=main.MAP_R) return;
if (c>=main.MAP_C) return;

this.setCell(r,c);

this.r = r;
this.c = c;

this.setXY(this.getX() + main.canvasScale_W/2, this.getY() + main.canvasScale_H/2);

]]></Body>
